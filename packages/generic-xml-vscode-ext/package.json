{
  "name": "generic-xml-vscode-ext",
  "description": "VSCode client part of an xml language server",
  "author": "SAP SE",
  "version": "0.0.1",
  "publisher": "vscode",
  "repository": {
    "type": "git",
    "url": "https://github.com/SAP/xml-tools"
  },
  "engines": {
    "vscode": "^1.39.0"
  },
  "activationEvents": [
    "onLanguage:xml"
  ],
  "main": "./out/extension",
  "scripts": {
    "vscode:prepublish": "npm run update-vscode && cd .. && npm run compile",
    "postinstall": "vscode-install",
    "compile": "tsc -b",
    "watch": "tsc -b -w"
  },
  "dependencies": {
    "vscode-languageclient": "^5.2.1"
  },
  "devDependencies": {
    "@types/node": "^12.11.7",
    "vscode": "^1.1.35",
    "typescript": "^3.6.4"
  }
}
